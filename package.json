{
  "name": "auctionbackend",
  "version": "1.0.0",
  "description": "A graphQL auction server",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "debug": "npx tsc && node --inspect dist/index.js",
    "generate": "graphql-codegen",
    "dev": "concurrently \"npx tsc --watch\" \"graphql-codegen\" \"nodemon --inspect index.js\""
  },
