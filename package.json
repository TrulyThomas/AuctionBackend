  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.17",
    "@prisma/client": "^4.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "concurrently": "^7.6.0",
    "graphql-codegen": "^0.4.0",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "typescript": "^4.9.5"
  }
